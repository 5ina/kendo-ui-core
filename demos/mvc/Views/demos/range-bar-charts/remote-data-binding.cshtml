<div id="example">
    <div class="demo-section k-content wide">
        <div id="chart"></div>
    </div>
    <script>
        function createChart() {
            $("#chart").kendoChart({
                dataSource: {
                    transport: {
                        read: {
                            url: "../content/dataviz/js/download-speed.json",
                            dataType: "json"
                        }
                    }
                },
                title: {
                    text: "Transfer speed Mbit/s"
                },
                legend: {
                    position: "top"
                },
                seriesDefaults: {
                    type: "rangeColumn"
                },
                series:
                [{
                    fromField: "WiFiFrom",
                    toField: "WiFiTo",
                    name: "WiFi"
                }, {
                    fromField: "OpticalFrom",
                    toField: "OpticalTo",
                    name: "Optical"
                }],
                categoryAxis: {
                    field: "day"
                },
                tooltip: {
                    visible: true,
                    template: "#= value.from # - #= value.to #"
                }
            });
        }

        $(document).ready(createChart);
        $(document).bind("kendo:skinChange", createChart);
    </script>
</div>
