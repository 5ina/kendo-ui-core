<div id="example" role="application">
    <div class="demo-section k-content">
        <h4 class="title">ListBox Events</h4>
        <label for="optional">Optional</label>
        <select id="optional" data-placeholder="Select attendees...">
            <option>Steven White</option>
            <option>Nancy King</option>
            <option>Nancy Davolio</option>
            <option>Robert Davolio</option>
            <option>Michael Leverling</option>
            <option>Andrew Callahan</option>
            <option>Michael Suyama</option>
        </select>
        <label for="selected">Selected</label>
        <select id="selected" multiple="multiple" data-placeholder="Selected attendees..."></select>
    </div>
    <div class="box">
        <h4>Console log</h4>
        <div class="console"></div>
    </div>
    <script>
        $(document).ready(function () {

            function onBeforeMove(e) {
                console.log(e);
                kendoConsole.log("event: beforeMove");
            };

            function onChange(e) {
                kendoConsole.log("event: change");
            };

            function onDataBound(e) {
                if ("kendoConsole" in window) {
                    kendoConsole.log("event :: dataBound");
                }
            };

            function onEnd(e) {
                console.log(e);
                kendoConsole.log("event: end");
            };

            function onMove(e) {
                console.log(e);
                kendoConsole.log("event: move");
            };

            function onRemove(e) {
                console.log(e);
                kendoConsole.log("event: remove");
            };

            function onReorder(e) {
                console.log(e);
                kendoConsole.log("event: reorder");
            };

            function onStart(e) {
                console.log(e);
                kendoConsole.log("event: start");
            };

            function onTransfer(e) {
                console.log(e);
                kendoConsole.log("event: transfer");
            };            

            $("#optional").kendoListBox({
                connectWith: "#selected",
                draggable:true,
                dataTextField: "ContactName",
                dataValueField: "CustomerID",
                dataSource: {
                    type: "odata",
                    transport: {
                        read: "https://demos.telerik.com/kendo-ui/service/Northwind.svc/Customers"
                    },
                    pageSize: 10
                },
                selectable: "multiple",
                beforeMove: onBeforeMove,
                change: onChange,
                dataBound: onDataBound,
                end: onEnd,
                move: onMove,
                remove: onRemove,
                reorder: onReorder,
                start: onStart,
                transfer: onTransfer
            });

            $("#selected").kendoListBox({
                connectWith: "#optional",
                draggable: true,
                dataTextField: "ContactName",
                dataValueField: "CustomerID",
                selectable: "multiple",
                beforeMove: onBeforeMove,
                change: onChange,
                dataBound: onDataBound,
                end: onEnd,
                move: onMove,
                remove: onRemove,
                reorder: onReorder,
                start: onStart,
                transfer: onTransfer
            });
        });
    </script>
</div>
