        <div id="example">
            <div class="demo-section k-content">
                <h4>Enter a date</h4>
                <input id="dateinput" style="width: 100%"/>
            </div>
            <div class="box">
                <h4>Console log</h4>
                <div class="console"></div>
            </div>
            <script>
                $(document).ready(function() {
                    function onChange() {
                        kendoConsole.log("Change :: " + kendo.toString(this.value(), 'd'));
                    }

                    $("#dateinput").kendoDateInput({
                        change: onChange,
                        value: new Date()
                    });
                });
            </script>            
        </div>
        
