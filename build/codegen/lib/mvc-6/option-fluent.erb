        /// <summary>
        /// <%= description.gsub(/\r?\n/, "\r\n\t\t/// ").html_encode()%>
        /// </summary>
        /// <param name="value">The value for <%= csharp_name %></param>
        public <%= csharp_owner_builder_name %> <%= csharp_name %>(<%= csharp_array? ? 'params ' : '' %><%= csharp_type %> value)
        {
            Container.<%= csharp_name %> = value;
            return this;
        }

%if template?
        /// <summary>
        /// <%= description.gsub(/\r?\n/, "\r\n\t\t/// ").html_encode()%>
        /// </summary>
        /// <param name="value">The ID of the template element for <%= csharp_name %></param>
        public <%= csharp_owner_builder_name %> <%= csharp_name %>Id(string templateId)
        {
            Container.<%= csharp_name %>Id = templateId;
            return this;
        }
%end
