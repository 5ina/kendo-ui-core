% if string_and_function?
% csharp_types_and_names.each do |type, propertyName|
% if !type.eql?("ClientHandlerDescriptor")
        /// <summary>
        /// <%= description.gsub(/\r?\n/, "\r\n\t\t/// ").html_encode()%>
        /// </summary>
        /// <param name="value">The value for <%= propertyName %></param>
        public <%= csharp_owner_builder_name %> <%= propertyName %>(<%= type %> value)
        {
            Container.<%= csharp_types_and_names["ClientHandlerDescriptor"] %> = null;
            Container.<%= propertyName %> = value;
            return this;
        }
%else
        /// <summary>
        /// <%= description.gsub(/\r?\n/, "\r\n\t\t/// ").html_encode()%>
        /// </summary>
        /// <param name="handler">The name of the JavaScript function that will be evaluated.</param>
        public <%= csharp_owner_builder_name %> <%= csharp_types_and_names[type] %>(string handler)
        {
% csharp_types_and_names.each do |type, propertyName|
% if !type.eql?("ClientHandlerDescriptor")
            Container.<%= csharp_types_and_names[type] %> = null;
% end
% end
            Container.<%= csharp_types_and_names[type] %> = new ClientHandlerDescriptor { HandlerName = handler };
            return this;
        }

        /// <summary>
        /// <%= description.gsub(/\r?\n/, "\r\n\t\t/// ").html_encode()%>
        /// </summary>
        /// <param name="handler">The handler code wrapped in a text tag.</param>
        public <%= csharp_owner_builder_name %> <%= csharp_types_and_names[type] %>(Func<object, object> handler)
        {
% csharp_types_and_names.each do |type, propertyName|
% if !type.eql?("ClientHandlerDescriptor")
            Container.<%= csharp_types_and_names[type] %> = null;
% end
% end
            Container.<%= csharp_types_and_names[type] %> = new ClientHandlerDescriptor { TemplateDelegate = handler };
            return this;
        }

% end
% end
% else
% if handler?
        /// <summary>
        /// <%= description.gsub(/\r?\n/, "\r\n\t\t/// ").html_encode()%>
        /// </summary>
        /// <param name="handler">The name of the JavaScript function that will be evaluated.</param>
        public <%= csharp_owner_builder_name %> <%= csharp_name %>(string handler)
        {
            Container.<%= csharp_name %> = new ClientHandlerDescriptor { HandlerName = handler };
            return this;
        }

        /// <summary>
        /// <%= description.gsub(/\r?\n/, "\r\n\t\t/// ").html_encode()%>
        /// </summary>
        /// <param name="handler">The handler code wrapped in a text tag.</param>
        public <%= csharp_owner_builder_name %> <%= csharp_name %>(Func<object, object> handler)
        {
            Container.<%= csharp_name %> = new ClientHandlerDescriptor { TemplateDelegate = handler };
            return this;
        }
% else
        /// <summary>
        /// <%= description.gsub(/\r?\n/, "\r\n\t\t/// ").html_encode()%>
        /// </summary>
        /// <param name="value">The value for <%= csharp_name %></param>
        public <%= csharp_owner_builder_name %> <%= csharp_name %>(<%= csharp_array? ? 'params ' : '' %><%= csharp_type %> value)
        {
            Container.<%= csharp_name %> = value;
            return this;
        }

% if dictionary?
        /// <summary>
        /// <%= description.gsub(/\r?\n/, "\r\n\t\t/// ").html_encode()%>
        /// </summary>
        /// <param name="value">The value for <%= csharp_name %></param>
        public <%= csharp_owner_builder_name %> <%= csharp_name %>(object value)
        {
            Container.<%= csharp_name %> = value.ToDictionary();
            return this;
        }

% end
% end
% if template?
        /// <summary>
        /// <%= description.gsub(/\r?\n/, "\r\n\t\t/// ").html_encode()%>
        /// </summary>
        /// <param name="value">The ID of the template element for <%= csharp_name %></param>
        public <%= csharp_owner_builder_name %> <%= csharp_name %>Id(string templateId)
        {
            Container.<%= csharp_name %>Id = templateId;
            return this;
        }

% end
% if csharp_type.eql?('bool') && (default.eql?('false') || default.eql?(false))
        /// <summary>
        /// <%= description.gsub(/\r?\n/, "\r\n\t\t/// ").html_encode()%>
        /// </summary>
        public <%= csharp_owner_builder_name %> <%= csharp_name %>()
        {
            Container.<%= csharp_name %> = true;
            return this;
        }

% end
% end
