using Kendo.Mvc.Extensions;
using System;

namespace Kendo.Mvc.UI
{
    /// <summary>
    /// Kendo UI I<%= type %> interface
    /// </summary>
    public interface I<%= type %>
    {
% members.select{ |o| o.public_interface.nil? }.each do |member|
        <%= member.csharp_type %> <%= member.name %> { get; set; }
% end
    }
}

