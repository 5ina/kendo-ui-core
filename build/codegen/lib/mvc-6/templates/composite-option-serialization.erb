%if serialize?
            var <%= name %> = <%= csharp_name %>.Serialize();
            if (<%= name %>.Any())
            {
                settings["<%= name %>"] = <%= name %>;
            }
% if toggleable
            else if (<%= csharp_name %>.Enabled == true)
            {
                settings["<%= name %>"] = true;
            }
%end
%end

