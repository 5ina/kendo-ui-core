using Kendo.Mvc.Extensions;
using Microsoft.AspNet.Mvc;
using System;
using System.Collections.Generic;
using System.Linq;

namespace Kendo.Mvc.UI
{
    /// <summary>
    /// Kendo UI <%= csharp_class %> component
    /// </summary>
    public partial class <%= csharp_class %><%= csharp_generic_args %> <%= csharp_generic_constraints %>
    {
<%= unique_options.map { |o| o.to_declaration }.join %>

        protected override Dictionary<string, object> SerializeSettings()
        {
            var settings = base.SerializeSettings();

% if prefix?
            InitializePrefix();
% end

<%= unique_options.map { |o| o.to_serialization }.join %>
            return settings;
        }

% if prefix?
        protected void InitializePrefix()
        {
            <%= composite_options.map { |o| o.to_prefix }.join %>
        }
% end
    }
}
