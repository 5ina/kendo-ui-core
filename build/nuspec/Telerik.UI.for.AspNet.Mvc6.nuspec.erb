<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
  <metadata>
    <id>Telerik.UI.for.AspNet.Mvc6</id>
    <version><%= VERSION %>-beta1</version>
    <title>Telerik UI for ASP.NET MVC 6</title>
    <authors>Telerik</authors>
    <owners>Telerik</owners>
